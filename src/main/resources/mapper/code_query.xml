<?xml version="1.0" encoding="UTF-8"?>
<!-- sql 작성 -->
<!DOCTYPE mapper PUBLIC "-//rest.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycom.springboot.code.dao.CodeDao">
	
	<select id="codeList" parameterType="String" resultType="com.mycom.springboot.code.dto.CodeDto">
		select code, code_name
		  from code
		 where group_code = #{groupCode};
	</select>
</mapper>